#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xquark_home/Faramesh-Nexus/openclaw-test/node_modules/.pnpm/openclaw@2026.2.21-2_@napi-rs+canvas@0.1.94_@types+express@5.0.6_audio-decode@2.2.3_hon_24c7ad19ff3298b56ddc96b4bb3d880e/node_modules/openclaw/node_modules:/Users/xquark_home/Faramesh-Nexus/openclaw-test/node_modules/.pnpm/openclaw@2026.2.21-2_@napi-rs+canvas@0.1.94_@types+express@5.0.6_audio-decode@2.2.3_hon_24c7ad19ff3298b56ddc96b4bb3d880e/node_modules:/Users/xquark_home/Faramesh-Nexus/openclaw-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xquark_home/Faramesh-Nexus/openclaw-test/node_modules/.pnpm/openclaw@2026.2.21-2_@napi-rs+canvas@0.1.94_@types+express@5.0.6_audio-decode@2.2.3_hon_24c7ad19ff3298b56ddc96b4bb3d880e/node_modules/openclaw/node_modules:/Users/xquark_home/Faramesh-Nexus/openclaw-test/node_modules/.pnpm/openclaw@2026.2.21-2_@napi-rs+canvas@0.1.94_@types+express@5.0.6_audio-decode@2.2.3_hon_24c7ad19ff3298b56ddc96b4bb3d880e/node_modules:/Users/xquark_home/Faramesh-Nexus/openclaw-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openclaw/openclaw.mjs" "$@"
else
  exec node  "$basedir/../openclaw/openclaw.mjs" "$@"
fi
